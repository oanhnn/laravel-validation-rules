{
  "name": "oanhnn/laravel-validation-rules",
  "type": "library",
  "description": "A validation rules collection",
  "keywords": [
    "laravel",
    "validation",
    "validate",
    "rules"
  ],
  "license": "MIT",
  "homepage": "https://github.com/oanhnn/laravel-validation-rules",
  "support": {
    "issues": "https://github.com/oanhnn/laravel-validation-rules/issues",
    "source": "https://github.com/oanhnn/laravel-validation-rules"
  },
  "authors": [
    {
      "name": "Oanh Nguyen",
      "email": "oanhnn.bk@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Laravel\\Validation\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "require": {
    "php": "^7.2.5",
    "illuminate/support": "^6.0|^7.0|^8.0"
  },
  "require-dev": {
    "orchestra/testbench": "^4.0|^5.0|^6.0",
    "phpunit/phpunit": "^8.0|^9.0"
  },
  "scripts": {
    "test": [
      "phpunit --testsuite=Tests --testdox"
    ],
    "coverage": [
      "phpunit --testsuite=Tests --coverage-html storage/coverage"
    ]
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "Laravel\\Validation\\ServiceProvider"
      ]
    }
  }
}
